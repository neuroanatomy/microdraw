{
  "name": "microdraw",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "debug": "node --inspect-brk ./bin/www",
    "lint": "eslint --ext .js,.vue .",
    "test": "cross-env MONGODB_TEST='127.0.0.1:27017/circleci_test' node test.js",
    "mocha": "cross-env MONGODB_TEST='127.0.0.1:27017/circleci_test' mocha test/mocha.test.js",
    "mocha-brk": "cross-env MONGODB_TEST='127.0.0.1:27017/circleci_test' mocha test/mocha.test.js --inspect-brk --timeout=10000000",
    "test-e2e": "cross-env MONGODB_TEST='127.0.0.1:27017/circleci_test' CRDT_BACKEND_PORT=58081 LOCALSIGNIN=true mocha ./test/e2e/runner.js ./test/e2e/*.spec.js --timeout=100000",
    "prepare": "husky install",
    "build-pages": "webpack --mode production --config webpack.pages.config.js && cp app/views/scripts/dist/*-page.js app/public/js/pages/",
    "dev-pages": "webpack --mode development --config webpack.pages.config.js && cp app/views/scripts/dist/*-page.js app/public/js/pages/"
  },
  "dependencies": {
    "@hocuspocus/provider": "^2.2.3",
    "@hocuspocus/server": "^2.2.3",
    "@syncedstore/core": "^0.4.3",
    "body-parser": "^1.18.3",
    "cross-env": "^7.0.3",
    "dateformat": "^3.0.2",
    "debug": "^4.1.1",
    "dompurify": "^2.2.6",
    "eslint-config-naat": "github:neuroanatomy/eslint-config-naat",
    "express": "^4.16.3",
    "jsdom": "^16.4.0",
    "monk": "^7.1.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "mustache-express": "latest",
    "neuroweblab": "github:neuroanatomy/neuroweblab",
    "nwl-components": "^1.3.11",
    "passport": "^0.4.0",
    "passport-github": "latest",
    "request": "^2.88.0",
    "serve-favicon": "latest",
    "vue": "^3.5.3",
    "y-webrtc": "^10.2.3",
    "yjs": "^13.5.41"
  },
  "main": "app.js",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^6.6.0",
    "eslint": "^8.9.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "html-webpack-plugin": "^5.5.0",
    "http-terminator": "^3.0.4",
    "husky": "^7.0.0",
    "jpeg-js": "^0.4.3",
    "lint-staged": "^13.0.3",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0",
    "proxyquire": "^2.1.3",
    "puppeteer": "^21.1.0",
    "sinon": "^7.3.1",
    "style-loader": "^3.3.1",
    "vue-loader": "^17.0.0",
    "webpack": "^5.69.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint --cache --fix"
  },
  "author": "Roberto Toro <rto@braincatalogue.org> (https://neuroanatomy.github.io)",
  "license": "GPL-3.0",
  "description": ""
}
